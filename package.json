{
  "name": "yaml-files",
  "version": "0.1.0",
  "description": "Include multiple yaml files in another yaml file for js-yaml.",
  "main": "index.js",
  "scripts": {
    "test": "ava test/test* --verbose"
  },
  "keywords": [
    "yaml",
    "files",
    "js-yaml"
  ],
  "eslintConfig": {
    "extends": [
      "standard"
    ],
    "env": {
      "es6": true
    },
    "rules": {
      "semi": [
        2,
        "always"
      ],
      "no-extra-semi": 2,
      "semi-spacing": [
        2,
        {
          "before": false,
          "after": true
        }
      ],
      "new-cap": [
        2,
        {
          "properties": false
        }
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/scisco/yaml-files"
  },
  "bugs": {
    "url": "https://github.com/scisco/yaml-files/issues"
  },
  "homepage": "https://github.com/scisco/yaml-files",
  "author": "scisco",
  "license": "ISC",
  "dependencies": {
    "js-yaml": "^3.6.1",
    "lodash.merge": "^4.6.0"
  },
  "devDependencies": {
    "ava": "^0.16.0",
    "eslint": "^2.9.0",
    "eslint-config-standard": "^5.3.1",
    "eslint-plugin-promise": "^1.1.0",
    "eslint-plugin-standard": "^1.3.2"
  },
  "engines": {
    "node": ">=4"
  }
}
